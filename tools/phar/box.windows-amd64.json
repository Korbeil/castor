{
  "base-path": "../../",
  "files-bin": [
    "tools/watcher/bin/watcher-windows.exe"
  ],
  "finder-bin": [
    {
      "name": "/.*\\.exe|.*\\.dll/",
      "in": "vendor/jolicode/jolinotif/bin"
    }
  ],
  "blacklist": [
    "castor",
    "LICENSE",
    "Test",
    "tools",
    "vendor/psr/event-dispatcher",
    "vendor/symfony/error-handler",
    "vendor/symfony/event-dispatcher-contracts",
    "vendor/symfony/http-foundation"
  ],
  "compactors": [
    "KevinGH\\Box\\Compactor\\Php"
  ],
  "compression": "GZ",
  "check-requirements": false,
  "git-version": "package_version",
  "main": "bin/castor",
  "output": "tools/phar/build/castor.windows-amd64.phar"
}
